# 資料庫配置
DATABASE_URL=mysql://crypto_user:crypto_password@localhost:3306/crypto_dashboard

#本地測試認證模式(跳過 OAuth，直接使用測試用戶進行開發和測試)
ENABLE_TEST_AUTH=true

# JWT 密鑰（至少 32 個字符）
JWT_SECRET=your_random_32_char_secret_key_here_min_32_chars

# Etherscan API Key
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# OAuth 配置（使用 Manus OAuth）
VITE_APP_ID=your_app_id
OAUTH_SERVER_URL=https://api.manus.im
VITE_OAUTH_PORTAL_URL=https://oauth.manus.im

# 前端配置
VITE_APP_TITLE=加密資產儀表板
VITE_APP_LOGO=/logo.svg
VITE_ANALYTICS_ENDPOINT=http://localhost:3000
VITE_ANALYTICS_WEBSITE_ID=default

# 其他配置
OWNER_NAME=Your Name
OWNER_OPEN_ID=your_open_id
VITE_FRONTEND_FORGE_API_KEY=your_key
VITE_FRONTEND_FORGE_API_URL=https://api.manus.im
BUILT_IN_FORGE_API_KEY=your_key
BUILT_IN_FORGE_API_URL=https://api.manus.im
